 {
    "did_you_mean": {"type": "string", "analyzer": "didYouMean"},
    "autocomplete": {"type": "string", "analyzer": "autocomplete"},
    "merged_topics": {"type": "string", "analyzer": "default"},
    "merged_spatial": {"type": "string", "analyzer": "default"},
    "merged_places": {"type": "string", "analyzer": "default"},
    "merged_objectProvides": {"type": "string", "analyzer": "default"},

    "cluster_id": {
      "type" : "string",
      "analyzer" : "none"
    },
    "cluster_name": {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean"]
    },
    "language" : {
      "type" : "string",
      "null_value" : "en",
      "analyzer" : "none"
    },
    "http://www.eea.europa.eu/portal_types#dataProvider":{
       "type" : "string",
       "copy_to" : "organisation"
     },
     "http://www.eea.europa.eu/portal_types#dataProcessor":{
       "type" : "string",
       "copy_to" : "organisation"
     },
    "organisation" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean"]
    },
    "http://www.eea.europa.eu/portal_types#topic" : {
      "type" : "string",
      "null_value" : "Various other issues",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean", "merged_topics"]
    },
    "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean"]
    },
    "http://www.eea.europa.eu/ontologies.rdf#objectProvides" : {
      "type" : "string",
      "null_value" : "Other",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean", "merged_objectProvides"]
    },
    "http://purl.org/dc/terms/spatial" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean", "merged_spatial"]
    },
    "places" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean", "merged_places"]
    },
    "http://purl.org/dc/terms/creator" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean"]
    },
    "http://www.eea.europa.eu/ontologies.rdf#hasWorkflowState" : {
      "type" : "string",
      "analyzer" : "none"
    },
    "http://purl.org/dc/terms/title" : {
      "type" : "string",
      "fields" : {
        "toindex" : {"type" : "string", "analyzer" : "default"},
        "index" : {"type" : "string", "analyzer" : "none"},
        "http://purl.org/dc/terms/title": {"type": "string", "index": "not_analyzed"}
      },
      "copy_to" : ["did_you_mean", "autocomplete"]
    },
    "year" : {
      "type" : "string",
      "null_value" : "Unknown",
      "analyzer" : "date2year"
    },
    "time_coverage" : {
      "type" : "integer",
      "analyzer" : "date2year"
    },
    "http://purl.org/dc/terms/format": {
      "type" : "string",
      "analyzer" : "none"
    },
    "label": {
      "type" : "string",
      "fields" : {
        "toindex" : {"type" : "string", "analyzer" : "default"},
        "index" : {"type" : "string", "analyzer" : "none"},
        "label": {"type": "string", "index": "not_analyzed"}
      },
      "copy_to" : ["did_you_mean"]
    },
    "text" : {
      "type" : "string",
      "analyzer": "default",
      "copy_to" : ["did_you_mean"]
    },
    "http://purl.org/dc/terms/references" : {
      "type" : "string",
      "analyzer" : "none",
      "copy_to" : ["did_you_mean"]
    }
}
